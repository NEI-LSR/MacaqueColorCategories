@article{abney_change_1909,
  title = {On the Change in Hue of Spectrum Colours by Dilution with White Light},
  author = {Abney, William De Wiveleslie},
  year = {1909},
  month = dec,
  journal = {Proceedings of the Royal Society of London. Series A, Containing Papers of a Mathematical and Physical Character},
  volume = {83},
