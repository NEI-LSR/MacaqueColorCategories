@article{hughes_time_2003,
  title = {Time Course of Adaptation to Stimuli Presented along Cardinal Lines in Color Space},
  author = {Hughes, Alan and DeMarco, Paul J.},
  year = {2003},
  month = dec,
  journal = {JOSA A},
  volume = {20},
  number = {12},
  pages = {2216--2227},
  publisher = {{Optical Society of America}},
  issn = {1520-8532},
  doi = {10.1364/JOSAA.20.002216},
  url = {https://www.osapublishing.org/josaa/abstract.cfm?uri=josaa-20-12-2216},
  urldate = {2020-04-15},
  abstract = {Visual sensitivity is a process that allows the visual system to maintain optimal response over a wide range of ambient light levels and chromaticities. Several studies have used variants of the probe\textendash flash paradigm to show that the time course of adaptation to abrupt changes in ambient luminance depends on both receptoral and postreceptoral mechanisms. Though a few studies have explored how these processes govern adaptation to color changes, most of this effort has targeted the L\textendash M-cone pathway. The purpose of our work was to use the probe\textendash flash paradigm to more fully explore light adaptation in both the L\textendash M- and the S-cone pathways. We measured sensitivity to chromatic probes presented after the onset of a 2-s chromatic flash. Test and flash stimuli were spatially coextensive 2\textdegree{} fields presented in Maxwellian view. Flash stimuli were presented as excursions from white and could extended in one of two directions along an equiluminant L\textendash M-cone or S-cone line. Probes were presented as excursions from the adapting flash chromaticity and could extend either toward the spectrum locus or toward white. For both color lines, the data show a fast and slow adaptation component, although this was less evident in the S-cone data. The fast and slow components were modeled as first- and second-site adaptive processes, respectively. We find that the time course of adaptation is different for the two cardinal pathways. In addition, the time course for S-cone stimulation is polarity dependent. Our results characterize the rapid time course of adaptation in the chromatic pathways and reveal that the mechanics of adaptation within the S-cone pathway are distinct from those in the L\textendash M-cone pathways.},
  copyright = {\&\#169; 2003 Optical Society of America},
  langid = {english},
  keywords = {Color spaces,Cone cells,Light wavelength,Retinal ganglion cells,Visual sensitivity,Visual system},
  file = {C\:\\Users\\cege-user\\Dropbox\\Zotero Storage\\Hughes_DeMarco_2003_Time course of adaptation to stimuli presented along cardinal lines in color.pdf}
}

