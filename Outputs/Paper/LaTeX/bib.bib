@article{qiu_induction_2005,
  title = {Induction of Photosensitivity by Heterologous Expression of Melanopsin},
  author = {Qiu, Xudong and Kumbalasiri, Tida and Carlson, Stephanie M. and Wong, Kwoon Y. and Krishna, Vanitha and Provencio, Ignacio and Berson, David M.},
