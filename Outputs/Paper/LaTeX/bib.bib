@article{tajima_population_2016-1,
  title = {Population {{Code Dynamics}} in {{Categorical Perception}}},
  author = {Tajima, Chihiro I. and Tajima, Satohiro and Koida, Kowa and Komatsu, Hidehiko and Aihara, Kazuyuki and Suzuki, Hideyuki},
  year = {2016},
  month = mar,
  journal = {Scientific Reports},
  volume = {6},
  number = {1},
  pages = {22536},
  publisher = {{Nature Publishing Group}},
  issn = {2045-2322},
  doi = {10.1038/srep22536},
  url = {https://www.nature.com/articles/srep22536},
  urldate = {2022-01-07},
  abstract = {Categorical perception is a ubiquitous function in sensory information processing and is reported to have important influences on the recognition of presented and/or memorized stimuli. However, such complex interactions among categorical perception and other aspects of sensory processing have not been explained well in a unified manner. Here, we propose a recurrent neural network model to process categorical information of stimuli, which approximately realizes a hierarchical Bayesian estimation on stimuli. The model accounts for a wide variety of neurophysiological and cognitive phenomena in a consistent framework. In particular, the reported complexity of categorical effects, including (i) task-dependent modulation of neural response, (ii) clustering of neural population representation, (iii) temporal evolution of perceptual color memory and (iv) a non-uniform discrimination threshold, are explained as different aspects of a single model. Moreover, we directly examine key model behaviors in the monkey visual cortex by analyzing neural population dynamics during categorization and discrimination of color stimuli. We find that the categorical task causes temporally-evolving biases in the neuronal population representations toward the focal colors, which supports the proposed model. These results suggest that categorical perception can be achieved by recurrent neural dynamics that approximates optimal probabilistic inference in the changing environment.},
  copyright = {2016 The Author(s)},
  langid = {english},
  keywords = {Human behaviour,Network models,Perception},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Human behaviour;Network models;Perception Subject\_term\_id: human-behaviour;network-models;perception},
  file = {C\:\\Users\\cege-user\\Dropbox\\Zotero Storage\\Tajima et al_2016_Population Code Dynamics in Categorical Perception.pdf}
}

@article{tajima_task-dependent_2017,
  title = {Task-Dependent Recurrent Dynamics in Visual Cortex},
  author = {Tajima, Satohiro and Koida, Kowa and Tajima, Chihiro I and Suzuki, Hideyuki and Aihara, Kazuyuki and Komatsu, Hidehiko},
  editor = {Latham, Peter},
