@article{smolders_higher_2012,
  title = {A Higher Illuminance Induces Alertness Even during Office Hours: {{Findings}} on Subjective Measures, Task Performance and Heart Rate Measures},
  shorttitle = {A Higher Illuminance Induces Alertness Even during Office Hours},
