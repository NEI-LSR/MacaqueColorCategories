@article{nemes_behavioural_2010,
  title = {A Behavioural Investigation of Human Visual Short Term Memory for Colour},
  author = {Nemes, V. A. and Parry, N. R. A. and McKeefry, D. J.},
  year = {2010},
  journal = {Ophthalmic and Physiological Optics},
  volume = {30},
