@article{allen_melanopsin_2017,
  title = {Melanopsin {{Contributions}} to the {{Representation}} of {{Images}} in the {{Early Visual System}}},
  author = {Allen, Annette E. and Storchi, Riccardo and Martial, Franck P. and Bedford, Robert A. and Lucas, Robert J.},
