@article{williams_category_2021,
  title = {Category Labels Do Not Improve Working Memory Performance for Ambiguous Shapes},
  author = {Williams, Lauren and Brady, Timothy and St{\"o}rmer, Viola},
  year = {2021},
  month = sep,
  journal = {Journal of Vision},
  volume = {21},
  number = {9},
