  author = {Pilorz, Violetta and Tam, Shu K. E. and Hughes, Steven and Pothecary, Carina A. and Jagannath, Aarti and Hankins, Mark W. and Bannerman, David M. and Lightman, Stafford L. and Vyazovskiy, Vladyslav V. and Nolan, Patrick M. and Foster, Russell G. and Peirson, Stuart N.},
  year = {2016},
  month = jun,
  journal = {PLOS Biology},
