@phdthesis{kesner_current_1991,
  title = {The Current State of Museum Exhibition Lighting},
  author = {Kesner, Christine Wilson},
  year = {1991},
  address = {{United States -- Pennsylvania}},
  url = {http://search.proquest.com/docview/303923213/abstract/329B2A3ACBD74C43PQ/1},
  urldate = {2016-02-10},
  abstract = {Little scientific evidence is available to document the state of museum exhibition lighting regarding occupant and decision maker needs or the mismatches (disparities) between needs and "reality" which exist in actual settings. Without knowledge of constituent needs or of the existing situation, decision making is ineffective and improvement is unlikely. Building diagnostics provides a framework for this project. The diagnostic process serves as an essential link in more closely matching building performance stated needs with "reality" (perceived and existing physical conditions). This work provides the essential research base for architectural decision making and formulation of appropriate strategies to improve museum lighting performance. First, needs of various constituencies are determined, then the problems associated with perceived and instrument measured "reality" are identified, and finally the mismatches between need and "reality" are highlighted. The study included a survey of 130 museum personnel and lighting designers and 427 museum visitors to determine the needs of these constituencies. The current status of exhibition lighting was assessed via a comprehensive evaluation using both objective (instrument-based) and subjective measures of lighting performance in 39 electrically-lit and daylit galleries from 22 museums throughout the United States. Appropriate univariate and multivariate statistical techniques were used to consolidate the data. Analysis of this sample revealed that the important stated needs of decision makers and visitors were not met in "reality". The major decision maker mismatch was daylight management as related to artifact preservation in daylit galleries and the major visitor mismatch was artifact appearance in electrically-lit galleries. Study results suggest the limited resources available should be directed toward minimizing glare, enhancing artifact appearance in terms of color richness and detail visibility, and improving the effectiveness of daylight management to protect artifacts as related to illuminance and relative humidity. Specific improvement strategies are identified.},
  copyright = {Copyright ProQuest, UMI Dissertations Publishing 1991},
  langid = {english},
  school = {Carnegie Mellon University},
  keywords = {Communication and the arts,lighting design},
  annotation = {00001},
  file = {C\:\\Users\\cege-user\\Dropbox\\Zotero Storage\\Kesner_1991_The current state of museum exhibition lighting.pdf}
}

