@article{siuda-krzywicka_what_2020,
  title = {What {{Cognitive Neurology Teaches Us}} about {{Our Experience}} of {{Color}}},
  author = {{Siuda-Krzywicka}, Katarzyna and Bartolomeo, Paolo},
  year = {2020},
