@phdthesis{shimozaki_adaptation_1994,
  title = {Adaptation and Simultaneous Contrast Effects on Lightness Judgments},
  author = {Shimozaki, Steven Sesshu},
  year = {1994},
  address = {{United States -- California}},
  url = {https://search.proquest.com/pqdtglobal/docview/304093765/abstract/4094217979884339PQ/13},
  urldate = {2019-11-08},
  abstract = {In a series of studies on lightness perception, subjects performed a discrimination between two simulated reflectances appearing under a simulated illumination modulating sinusoidally. By manipulating the period of modulation, the time dependence of the lightness judgment was measured. If the modulations of illumination are faster than the operation of a mechanism responsible for lightness perception, the mechanism cannot compensate for the modulation, and lightness discrimination performance should suffer. Thus, the time course of lightness perception can be assessed by measuring increases of performance as the period of modulation increases. There are three classes of mechanisms hypothesized to account for lightness perception which each have specific time courses; multiplicative light adaptation (fast, 50 to 200 ms), subtractive light adaptation (fast and slow, up to 30 sec), and simultaneous contrast (instantaneous). Four main experiments were performed. To separate the effect of simultaneous contrast from light adaptation, targets were placed either adjacent to the background (Direct Contrast), or with a 1 degree dark annulus between the target and the background (Annulus); the annulus removes simultaneous contrast cues. Also, the targets were either increments or decrements relative to the background. Thus, the four experiments are Direct Contrast Increment, Direct Contrast Decrement, Annulus Increment, and Annulus Decrement. Results show that simultaneous contrast is the predominant cue for lightness perception in these experiments. A control study comparing Annulus and Direct Contrast conditions for fixed illuminations also suggest strongly the importance of simultaneous contrast. Without simultaneous contrast cues (Annulus), small effects upon lightness perception occur for periods up to 120 sec. These long-term effects most likely correspond to slow adaptation mechanisms (subtractive). No difference was found between increment and decrement targets.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  langid = {english},
  school = {University of California, Los Angeles},
  keywords = {Psychology},
  file = {C\:\\Users\\cege-user\\Dropbox\\Zotero Storage\\Shimozaki_1994_Adaptation and simultaneous contrast effects on lightness judgments.pdf}
}

