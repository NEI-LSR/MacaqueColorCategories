@article{huang_visual_2019,
  title = {A {{Visual Circuit Related}} to {{Habenula Underlies}} the {{Antidepressive Effects}} of {{Light Therapy}}},
  author = {Huang, Lu and Xi, Yue and Peng, Yanfang and Yang, Yan and Huang, Xiaodan and Fu, Yunwei and Tao, Qian and Xiao, Jia and Yuan, Tifei and An, Kai and Zhao, Huan and Pu, Mingliang and Xu, Fuqiang and Xue, Tian and Luo, Minmin and So, Kwok-Fai and Ren, Chaoran},
