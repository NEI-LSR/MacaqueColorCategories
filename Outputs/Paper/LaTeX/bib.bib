  copyright = {Copyright \textcopyright{} 1997 Society for Neuroscience},
  langid = {english},
  pmid = {9151748},
  keywords = {area MT,area V5,middle temporal visual area,motion processing,pursuit initiation,saccades,saccadic target velocity compensation,vector averaging,vector summation,velocity,visual coding,winner-take-all; smooth pursuit},
