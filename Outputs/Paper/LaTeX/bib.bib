@article{longcore_tuning_2015,
  title = {Tuning the White Light Spectrum of Light Emitting Diode Lamps to Reduce Attraction of Nocturnal Arthropods},
  author = {Longcore, Travis and Aldern, Hannah L. and Eggers, John F. and Flores, Steve and Franco, Lesly and {Hirshfield-Yamanishi}, Eric and Petrinec, Laina N. and Yan, Wilson A. and Barroso, Andr{\'e} M.},
