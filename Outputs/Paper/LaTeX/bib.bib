@phdthesis{forsyth_colour_1989,
  type = {D.{{Phil}}.},
  title = {Colour Constancy and Its Applications in Machine Vision},
  author = {Forsyth, D. A.},
  year = {1989},
  address = {{England}},
  url = {https://search.proquest.com/docview/301454428},
  urldate = {2019-02-25},
  abstract = {Colour constancy is a skill by which people can tell the`colour of an object correctly, even when it is illuminated by strongly coloured lights. I derive the colour constancy equation, and obtain from this derivation the circumstances under which colour constancy is possible. I then employ the properties of surface reflectance functions to turn colour constancy into a parameter estimation problem. This leads in a natural way to two novel algorithms for colour constancy, distinguished by the assumptions required of the world for their success. The first, called Crule, succeeds on all surface reflectances under restrictions on the illuminant and corresponds to normalisation according to a coefficient rule. A physical realisability argument provides a strong constraint on the coefficients for this kind of normalisation. Within the region that satisfies this constraint, an estimator is used to select a set of coefficients for predicting colour appearance. The second algorithm, called Mwext, although a natural extension of recent work of Maloney and Wandell, is complex and restrictive in its requirements of the world. I show that these algorithms achieve colour constancy on real pictures. I show that this formalism is capable of reproducing qualitative aspects of Land's two colour experiments. I compare the Crule algorithm with a model of the Retinex algorithm of Land and show that the descriptors computed by the Crule algorithm are not as strongly dependent on the structure of the scene as those computed by the Retinex algorithm. Finally, I show that colour constancy can improve the quality of image segmentation, and is hence a useful and important technique for machine vision. A comprehensive review of the literature treating colour constancy in machine and human vision is provided.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  langid = {english},
  school = {University of Oxford (United Kingdom)},
  keywords = {(UMI)AAIU021783,Psychology}
}

