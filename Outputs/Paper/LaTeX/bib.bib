@article{rosenthal_color_2021,
  title = {Color {{Space Geometry Uncovered}} with {{Magnetoencephalography}}},
  author = {Rosenthal, Isabelle A. and Singh, Shridhar R. and Hermann, Katherine L. and Pantazis, Dimitrios and Conway, Bevil R.},
  year = {2021},
