@article{smithson_sensory_2005,
  title = {Sensory, Computational and Cognitive Components of Human Colour Constancy},
  author = {Smithson, H. E.},
  year = {2005},
  month = jun,
  journal = {Philosophical Transactions of the Royal Society of London B: Biological Sciences},
  volume = {360},
