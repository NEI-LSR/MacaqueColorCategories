@article{delahunt_long-term_2004,
  title = {Long-Term Renormalization of Chromatic Mechanisms Following Cataract Surgery},
  author = {Delahunt, Peter B. and Webster, Michael A. and Ma, Lei and Werner, John S.},
  year = {2004},
  month = may,
  journal = {Visual Neuroscience},
  volume = {21},
