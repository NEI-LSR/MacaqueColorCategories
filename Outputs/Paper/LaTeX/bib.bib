@article{bramao_role_2011,
  title = {The Role of Color Information on Object Recognition: {{A}} Review and Meta-Analysis},
  shorttitle = {The Role of Color Information on Object Recognition},
  author = {Bram{\~a}o, In{\^e}s and Reis, Alexandra and Petersson, Karl Magnus and Fa{\'i}sca, Lu{\'i}s},
  year = {2011},
  month = sep,
  journal = {Acta Psychologica},
