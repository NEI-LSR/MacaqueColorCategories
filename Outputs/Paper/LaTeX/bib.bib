  langid = {english},
  pmid = {32355031},
  file = {C\:\\Users\\cege-user\\Dropbox\\Zotero Storage\\Sonoda et al_2020_A noncanonical inhibitory circuit dampens behavioral sensitivity to light.pdf}
}

