@article{kupers_modeling_2019,
  title = {Modeling Visual Performance Differences `around' the Visual Field: {{A}} Computational Observer Approach},
  shorttitle = {Modeling Visual Performance Differences `around' the Visual Field},
  author = {Kupers, Eline R. and Carrasco, Marisa and Winawer, Jonathan},
  year = {2019},
  month = may,
  journal = {PLOS Computational Biology},
  volume = {15},
  number = {5},
  pages = {e1007063},
  publisher = {{Public Library of Science}},
  issn = {1553-7358},
  doi = {10.1371/journal.pcbi.1007063},
  url = {https://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1007063},
  urldate = {2020-07-14},
  abstract = {Visual performance depends on polar angle, even when eccentricity is held constant; on many psychophysical tasks observers perform best when stimuli are presented on the horizontal meridian, worst on the upper vertical, and intermediate on the lower vertical meridian. This variation in performance `around' the visual field can be as pronounced as that of doubling the stimulus eccentricity. The causes of these asymmetries in performance are largely unknown. Some factors in the eye, e.g. cone density, are positively correlated with the reported variations in visual performance with polar angle. However, the question remains whether these correlations can quantitatively explain the perceptual differences observed `around' the visual field. To investigate the extent to which the earliest stages of vision\textendash optical quality and cone density\textendash contribute to performance differences with polar angle, we created a computational observer model. The model uses the open-source software package ISETBIO to simulate an orientation discrimination task for which visual performance differs with polar angle. The model starts from the photons emitted by a display, which pass through simulated human optics with fixational eye movements, followed by cone isomerizations in the retina. Finally, we classify stimulus orientation using a support vector machine to learn a linear classifier on the photon absorptions. To account for the 30\% increase in contrast thresholds for upper vertical compared to horizontal meridian, as observed psychophysically on the same task, our computational observer model would require either an increase of \textasciitilde 7 diopters of defocus or a reduction of 500\% in cone density. These values far exceed the actual variations as a function of polar angle observed in human eyes. Therefore, we conclude that these factors in the eye only account for a small fraction of differences in visual performance with polar angle. Substantial additional asymmetries must arise in later retinal and/or cortical processing.},
  langid = {english},
  keywords = {Eye movements,Human performance,Optics,Photons,Photoreceptors,Psychophysics,Retina,Vision},
  file = {C\:\\Users\\cege-user\\Dropbox\\Zotero Storage\\Kupers et al_2019_Modeling visual performance differences ‘around’ the visual field.pdf}
}

