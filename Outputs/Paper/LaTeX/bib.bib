@article{mackey_framework_2019,
  title = {A {{Framework Proposal}} for {{Blockchain-Based Scientific Publishing Using Shared Governance}}},
  author = {Mackey, Tim K. and Shah, Neal and Miyachi, Ken and Short, James and Clauson, Kevin},
  year = {2019},
  journal = {Frontiers in Blockchain},
