@article{meltzer_retinal_2017,
  title = {Retinal {{Architecture}} and {{Melanopsin-Mediated Pupillary Response Characteristics}}: {{A Putative Pathophysiologic Signature}} for the {{Retino-Hypothalamic Tract}} in {{Multiple Sclerosis}}},
  shorttitle = {Retinal {{Architecture}} and {{Melanopsin-Mediated Pupillary Response Characteristics}}},
  author = {Meltzer, Ethan and Sguigna, Peter V. and Subei, Adnan and Beh, Shin and Kildebeck, Eric and Conger, Darrel and Conger, Amy and Lucero, Marlen and Frohman, Benjamin S. and Frohman, Ashley N. and Saidha, Shiv and Galetta, Steven and Calabresi, Peter A. and Rennaker, Robert and Frohman, Teresa C. and Kardon, Randy H. and Balcer, Laura J. and Frohman, Elliot M.},
  year = {2017},
  month = may,
  journal = {JAMA Neurology},
  volume = {74},
