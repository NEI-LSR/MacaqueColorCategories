@article{hunt_molecular_1998,
  title = {Molecular Evolution of Trichromacy in Primates},
  author = {Hunt, David M and Dulai, Kanwaljit S and Cowing, Jill A and Julliot, Catherine and Mollon, John D and Bowmaker, James K and Li, Wen-Hsiung and {Hewett-Emmett}, David},
  year = {1998},
  month = nov,
  journal = {Vision Research},
  volume = {38},
